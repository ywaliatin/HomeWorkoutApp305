<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.HomeWorkout"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>

        <!-- Add the following activity declarations for each workout activity class -->
        <activity
            android:name=".PushUpActivity" />
        <activity
            android:name=".SitUpActivity" />

        <activity android:name=".SquatActivity" />
        <activity android:name=".BurpeesActivity" />
        <activity android:name=".JumpingJackActivity" />
        <activity android:name=".PlankActivity" />
        <activity android:name=".Timer" />
        <activity android:name=".TotalActivity"/>


    </application>

</manifest>
